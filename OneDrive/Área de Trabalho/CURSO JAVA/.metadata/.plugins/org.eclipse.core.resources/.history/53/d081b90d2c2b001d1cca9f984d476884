		System.out.println("==============================================");
		System.out.println("                   INÍCIO");
		int x;
		double c,y;
		c=5;
		y= 2 * c;
		System.out.println(y);
		
		
		//EXEMPLO 3
		//Calculando a área de um trapézio
		System.out.println("==============================================");
		System.out.println("                  Exemplo 3");
		double b, B, h, area;
		b = 6.0;
		B = 8.0;
		h = 5.0;
		
		area = ((b+B)/2.0)*h;
		
		System.out.println(area);
		
		
		//EXEMPLO 4
		System.out.println("==============================================");
		System.out.println("                  Exemplo 4");
		
